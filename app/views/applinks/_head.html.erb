<% applinks_builder.android do |app| %>
<% if app.versioned %>
<meta property="al:android" />
<% end %>
<meta property="al:android:url" content="<%= app.url %>" />
<% if app.app_name %>
<meta property="al:android:app_name" content="<%= app.app_name %>" />
<% end %>
<% if app.package %>
